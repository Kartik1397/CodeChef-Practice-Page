{
    "require": {
        "php-di/php-di": "^6.3",
        "phpmailer/phpmailer": "^6.1",
        "rbdwllr/psr-jwt": "^1.0",
        "selective/basepath": "^1.0",
        "slim/psr7": "^1.2",
        "slim/slim": "4.*",
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
